<div class="wrapper">
  <div class="container">
    <div class="forms">
      <%= form_with model: @profile, local: true do |f| %>
        <h2>プロフィール登録</h2>
        <%= render "devise/shared/error_messages", resource: @profile %>
        <%= render "shared/profile_form", f: f %>
        <div class="actions">
        <%= f.submit "新規登録" %>
        </div>
      <% end %>
    </div>
  </div>
</div>
